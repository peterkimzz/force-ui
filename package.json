{
  "name": "force-ui",
  "version": "0.1.0",
  "description": "Web UI Components for Vue.js",
  "main": "dist/force-ui.js",
  "style": "dist/force.ui.css",
  "scripts": {
    "dev": "parcel -p 3000 watch -d app example/index.html",
    "build": "parcel build -d dist src/index.js --out-file force-ui",
    "build-vue-cli": "vue-cli-service build --dest dist2 --target lib --name force-ui src/index.js",
    "publish": "yarn build && npm publish"
  },
  "files": [
    "dist/*",
    "*.json"
  ],
  "author": "peterkimzz <peterkimzz69@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/peterkimzz/force-ui.git"
  },
  "homepage": "https://github.com/peterkimzz/force-ui.git",
  "bugs": {
    "url": "https://github.com/peterkimzz/force-ui/issues",
    "email": "peterkimzz69@gmail.com"
  },
  "dependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli": "^4.4.6",
    "@vue/cli-service-global": "^4.4.6",
    "parcel-bundler": "^1.12.4",
    "rollup-plugin-vue": "^5.1.9",
    "tailwindcss": "^1.4.6",
    "vue-template-compiler": "^2.6.11"
  }
}
